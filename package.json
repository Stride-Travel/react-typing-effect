{
  "name": "react-typing-effect",
  "version": "1.0.1",
  "description": "Simulates typing effect, eraser effect and blinking cursor.",
  "main": "build/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src/lib -d build",
    "build:watch": "babel src/lib -w -d build",
    "start-server": "babel-node tools/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* tools src",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"src/_tests/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "start": "npm-run-all --parallel build:watch test:watch start-server lint:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lamyfarai/react-typing-effect.git"
  },
  "author": "L.Farai Matare <lameckfarai@gmail.com> (http://lameckmatare.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lamyfarai/react-typing-effect/issues"
  },
  "homepage": "https://github.com/lamyfarai/react-typing-effect#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "eslint": "^4.14.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^22.0.3",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "jsdom": "^9.5.0",
    "mocha": "^4.1.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.1.3",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.8.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1"
  }
}
